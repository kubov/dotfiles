	return
fi

# envs
export SBCL_HOME=/usr/lib64/sbcl/
# golang
export GOPATH=/home/kubov/code/golang

# Intel IPP
export IPP_INC=/opt/intel/composerxe/ipp/include
export IPP_SHARED="/opt/intel/ipp/lib/intel64/:/opt/intel/lib/intel64/"
